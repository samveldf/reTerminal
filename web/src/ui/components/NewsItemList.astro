---
import NewsItem from '../parts/NewsItem.astro';

interface Props {
  items: Array<{
    title: string;
    url: string;
    pubDate?: string;
  }>;
}

const { items } = Astro.props;
---

<div class="flex flex-col gap-2">
  {items.map((item) => <NewsItem title={item.title} url={item.url} />)}
</div>
